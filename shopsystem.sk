      set {이전재고::대구} to {재고::대구}
      set {이전재고::연어} to {재고::연어}
      set {이전재고::복어} to {재고::복어}
      set {이전재고::흰동가리} to {재고::흰동가리}


command /상점초기화:
  trigger:
    if player is op:
      set {재고::밀} to 0
      set {판매가격::밀} to 30
      set {구매가격::밀} to 55
      set {판매가격::호박} to 20
      set {구매가격::호박} to 40
      set {구매가격::당근} to 70
      set {판매가격::당근} to 77
      set {판매가격::감자} to 70
      set {구매가격::감자} to 77
      set {판매가격::석탄} to 70
      set {구매가격::석탄} to 77
      set {판매가격::철} to 100
      set {구매가격::철} to 140
