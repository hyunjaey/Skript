every 1 minutes:
  add 10 to {재고::밀}
  add 10 to {재고::호박}
  add 10 to {재고::감자}
  add 10 to {재고::당근}
  add 10 to {재고::사탕수수}
  add 10 to {재고::달콤한열매}
  add 10 to {재고::석탄}
  add 10 to {재고::철}
  add 10 to {재고::금}
  add 1 to {재고::다이아몬드}
  add 10 to {재고::대구}
  add 10 to {재고::연어}
  add 10 to {재고::복어}
  add 10 to {재고::흰동가리}
  execute command "/변동"
command /convtoxp [<number>]:
  trigger:
    if {돈::%player%} >= arg 1:
      subtract arg 1 from {돈::%player%}
      set {_price::%player%} to arg 1 * 0.05
      add {_price::%player%} to xp of player
      message "test" to player
    else:
      message "&c%arg 1 - {돈::%player%}%원만큼의 돈이 부족합니다!" to player
command /돈확인:
  trigger:
    send "&f현재 %player%님의 소지금액은 <orange>%{돈::%player%}%원 &f입니다." to player
command /wallet:
  trigger:
    execute player command "/돈확인"
command /돈보내기 [<player>] [<number>]:
  trigger:
    if arg 1 is not set:
      message "명령어 사용법: /돈보내기 [송금대상] [송금액]"
    if arg 2 is not set:
      message "명령어 사용법: /돈보내기 [송금대상] [송금액]"
    if {돈::%player%} >= arg 2:
      send "&c%arg 1%&f님에게 &c%arg 2%&f원을 송금하였습니다." to player
      subtract arg 2 from {돈::%player%}
      add arg 2 to {돈::%arg 1%}
      send "&c%player%&f님이 당신에게 &c%arg 2%&f원을 송금하였습니다." to arg 1
    else:
      send "&c돈이 부족합니다!" to player
command /pay [<player>] [<number>]:
  trigger:
    execute player command "/돈보내기 %arg 1% %arg 2%"
command /돈획득 [<number>]:
  trigger:
    if player is op:
      add arg 1 to {돈::%player%}

command /변동:
  trigger:
    if player is op:
    # 밀
      # 변동계수 조정 (상수 조절)
      set {판매가격::밀} to 20 - ({재고::밀} - {이전재고::밀})/4 + random number between -5 and 5
      # 최소가격 조정 (상수 조절)
      if {판매가격::밀} < 10:
        set {판매가격::밀} to 10
      # 최대가격 조정 (상수 조절)
      if {판매가격::밀} > 70:
        set {판매가격::밀} to 70
      set {구매가격::밀} to {판매가격::밀} * 1.1
      set {이전재고::밀} to {재고::밀}
    # 호박
      # 변동계수 조정 (상수 조절)
      set {판매가격::호박} to 20 - ({재고::호박} - {이전재고::호박})/5 + random number between -5 and 5
      # 최소가격 조정 (상수 조절)
      if {판매가격::호박} < 4:
        set {판매가격::호박} to 4
      # 최대가격 조정 (상수 조절)
      if {판매가격::호박} > 50:
        set {판매가격::호박} to 50
      set {구매가격::호박} to {판매가격::호박} * 2
    # 당근
        # 변동계수 조정 (상수 조절)
      set {판매가격::당근} to 70 - ({재고::당근} - {이전재고::당근}) * 1.2 + random number between -20 and 20
